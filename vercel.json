{
	"version": 2,
	"functions": {
		"api/**/*.[jt]s": {
			"runtime": "nodejs24.x",
			"maxDuration": 10
		},
		"api/*.[jt]s": {
			"runtime": "nodejs24.x",
			"memory": 128,
			"maxDuration": 10
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api/$1"
		}
	],
	"redirects": [
		{
			"source": "/",
			"destination": "https://github.com/anuraghazra/leetcode-readme-stats",
			"statusCode": 302
		}
	]
}