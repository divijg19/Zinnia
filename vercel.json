{
	"version": 2,
	"builds": [
		{
			"src": "api/health.ts",
			"use": "@vercel/node"
		},
		{
			"src": "api/stats.ts",
			"use": "@vercel/node"
		},
		{
			"src": "api/top-langs.ts",
			"use": "@vercel/node"
		},
		{
			"src": "streak/api/index.ts",
			"use": "@vercel/node"
		},
		{
			"src": "trophy/api/index.ts",
			"use": "@vercel/node"
		},
		{
			"src": "leetcode/api/index.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/health$",
			"dest": "api/health.ts"
		},
		{
			"src": "/api/stats$",
			"dest": "api/stats.ts"
		},
		{
			"src": "/api/top-langs$",
			"dest": "api/top-langs.ts"
		},
		{
			"src": "/api/streak$",
			"dest": "streak/api/index.ts"
		},
		{
			"src": "/api/trophy$",
			"dest": "trophy/api/index.ts"
		},
		{
			"src": "/api/leetcode$",
			"dest": "leetcode/api/index.ts"
		}
	]
}