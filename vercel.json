{
	"version": 2,
	"functions": {
		"trophy/api/**": {
			"runtime": "nodejs20.x"
		},
		"stats/api/**": {
			"runtime": "nodejs20.x"
		},
		"streak/api/**": {
			"runtime": "nodejs20.x"
		},
		"leetcode/api/**": {
			"runtime": "nodejs20.x"
		},
		"github/api/**": {
			"runtime": "nodejs20.x"
		}
	},
	"routes": [
		{
			"src": "/trophy/(.*)",
			"dest": "trophy/api/index.ts"
		},
		{
			"src": "/trophy$",
			"dest": "trophy/api/index.ts"
		},
		{
			"src": "/stats$",
			"dest": "stats/api/index.js"
		},
		{
			"src": "/stats/top-langs$",
			"dest": "stats/api/top-langs.js"
		},
		{
			"src": "/streak/(.*)",
			"dest": "streak/api/index.ts"
		},
		{
			"src": "/streak$",
			"dest": "streak/api/index.ts"
		},
		{
			"src": "/leetcode/(.*)",
			"dest": "leetcode/api/index.ts"
		},
		{
			"src": "/leetcode$",
			"dest": "leetcode/api/index.ts"
		},
		{
			"src": "/github/(.*)",
			"dest": "github/api/index.ts"
		},
		{
			"src": "/api/trophy/(.*)",
			"dest": "trophy/api/index.ts"
		},
		{
			"src": "/api/trophy$",
			"dest": "trophy/api/index.ts"
		},
		{
			"src": "/api/stats$",
			"dest": "stats/api/index.js"
		},
		{
			"src": "/api/stats/top-langs$",
			"dest": "stats/api/top-langs.js"
		},
		{
			"src": "/api/streak/(.*)",
			"dest": "streak/api/index.ts"
		},
		{
			"src": "/api/streak$",
			"dest": "streak/api/index.ts"
		},
		{
			"src": "/api/leetcode/(.*)",
			"dest": "leetcode/api/index.ts"
		},
		{
			"src": "/api/leetcode$",
			"dest": "leetcode/api/index.ts"
		},
		{
			"src": "/api/github/(.*)",
			"dest": "github/api/index.ts"
		}
	]
}