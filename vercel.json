{
	"version": 2,
	"functions": {
		"api/**/*.[jt]s": {
			"runtime": "nodejs24.x",
			"maxDuration": 10
		},
		"api/*.[jt]s": {
			"runtime": "nodejs24.x",
			"memory": 128,
			"maxDuration": 10
		}
	},
	"routes": [
		{
			"src": "/api/health$",
			"dest": "api/health.ts"
		},
		{
			"src": "/api/stats$",
			"dest": "api/stats.ts"
		},
		{
			"src": "/api/top-langs$",
			"dest": "api/top-langs.ts"
		},
		{
			"src": "/api/streak$",
			"dest": "api/streak.ts"
		},
		{
			"src": "/api/trophy$",
			"dest": "api/trophy.ts"
		},
		{
			"src": "/api/leetcode$",
			"dest": "api/leetcode.ts"
		},
		{
			"src": "/stats$",
			"dest": "api/stats.ts"
		},
		{
			"src": "/stats/top-langs$",
			"dest": "api/top-langs.ts"
		},
		{
			"src": "/api/stats/top-langs$",
			"dest": "api/top-langs.ts"
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api/$1"
		}
	],
	"redirects": [
		{
			"source": "/",
			"destination": "https://github.com/anuraghazra/leetcode-readme-stats",
			"statusCode": 302
		}
	]
}
