{
	"version": 2,
	"functions": {
		"trophy/api/**/*.ts": {
			"runtime": "nodejs18.x"
		},
		"stats/api/**/*.ts": {
			"runtime": "nodejs18.x"
		},
		"streak/api/**/*.ts": {
			"runtime": "nodejs18.x"
		},
		"leetcode/api/**/*.ts": {
			"runtime": "nodejs18.x"
		},
		"github/api/**/*.ts": {
			"runtime": "nodejs18.x"
		}
	},
	"builds": [
		{
			"src": "trophy/api/**/*.ts",
			"use": "@vercel/node"
		},
		{
			"src": "stats/api/**/*.ts",
			"use": "@vercel/node"
		},
		{
			"src": "streak/api/**/*.ts",
			"use": "@vercel/node"
		},
		{
			"src": "leetcode/api/**/*.ts",
			"use": "@vercel/node"
		},
		{
			"src": "github/api/**/*.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/trophy/(.*)",
			"dest": "trophy/api/index.ts"
		},
		{
			"src": "/stats/(.*)",
			"dest": "stats/api/index.ts"
		},
		{
			"src": "/streak/(.*)",
			"dest": "streak/api/index.ts"
		},
		{
			"src": "/leetcode/(.*)",
			"dest": "leetcode/api/index.ts"
		},
		{
			"src": "/github/(.*)",
			"dest": "github/api/index.ts"
		},
		{
			"src": "/api/trophy/(.*)",
			"dest": "trophy/api/index.ts"
		},
		{
			"src": "/api/stats/(.*)",
			"dest": "stats/api/index.ts"
		},
		{
			"src": "/api/streak/(.*)",
			"dest": "streak/api/index.ts"
		},
		{
			"src": "/api/leetcode/(.*)",
			"dest": "leetcode/api/index.ts"
		},
		{
			"src": "/api/github/(.*)",
			"dest": "github/api/index.ts"
		}
	]
}