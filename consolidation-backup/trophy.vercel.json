{
    "//": "Unused in monorepo: deployments use root vercel.json. Kept for local dev only.",
    "functions": {
        "api/**/*.[jt]s": {
            "runtime": "nodejs18.x",
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "./(.*)",
            "destination": "/api/$1"
        }
    ]
}