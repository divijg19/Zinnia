{
	"name": "zinnia",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"packageManager": "^bun@1.3.0",
	"scripts": {
		"dev": "bun run --watch index.ts",
		"build": "bun build index.ts --outdir=dist",
		"vercel": "vercel dev",
		"vercel:deploy": "bunx vercel --prod --token $VERCEL_TOKEN --yes",
		"lint": "biome check . --ext .ts,.js --rules",
		"format": "biome format . --ext .ts,.js",
		"type-check": "tsc --noEmit",
		"test:stats": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config ./stats/jest.config.js --runInBand -u",
		"test:leetcode": "cd leetcode && bunx vitest --run --coverage --coverage.include packages/*/src",
		"test": "npm run test:stats && npm run test:leetcode"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/bun": "^1.3.0",
		"@types/jest": "^30.0.0",
		"@vercel/node": "^5.4.0",
		"@vitest/coverage-v8": "3.2.4",
		"esbuild": "^0.25.11",
		"jest": "^30.2.0",
		"ts-jest": "^29.4.5",
		"typescript": "^5.9.3",
		"vercel": "^48.3.0",
		"vitest": "3.2.4",
		"wrangler": "^4.43.0"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	}
}
